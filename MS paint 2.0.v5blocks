\n\n\n\n\n// Generated code.\n\n// Include the V5 Library\n#include \"vex.h\"\n  \n// Allows for easier use of the VEX Library\nusing namespace vex;\n\nfloat Mode, coller;\n\nevent message1 = event();\n\n// \"when started\" hat block\nint whenStarted1() {\n  Mode = 2.0;\n  message1.broadcast();\n  return 0;\n}\n\n// \"when Brain screen pressed\" hat block\nvoid onevent_Brain_screen_pressed_0() {\n  if (Mode == 2.0) {\n    if (Brain.Screen.xPosition() > 250.0) {\n      if (Brain.Screen.yPosition() > 125.0) {\n        // Blue Box Pressed\n        Brain.Screen.clearScreen();\n        coller = 1.0;\n        Mode = 1.0;\n      }\n      else {\n        // Green Box Pressed\n        Brain.Screen.clearScreen();\n        coller = 2.0;\n        Mode = 1.0;\n      }\n    }\n    else {\n      if (Brain.Screen.yPosition() > 125.0) {\n        // Red Box Pressed\n        Brain.Screen.clearScreen();\n        coller = 3.0;\n        Mode = 1.0;\n      }\n      else {\n        // White Box Pressed\n        Brain.Screen.clearScreen();\n        coller = 4.0;\n        Mode = 1.0;\n      }\n    }\n  }\n}\n\n// \"when I receive message1\" hat block\nvoid onevent_message1_0() {\n  waitUntil((Mode == 2.0));\n  Brain.Screen.clearScreen();\n  Brain.Screen.setPenColor(white);\n  Brain.Screen.setFillColor(white);\n  Brain.Screen.drawRectangle(30, 10, 200, 100);\n  Brain.Screen.setFillColor(red);\n  Brain.Screen.drawRectangle(30, 125, 200, 100);\n  Brain.Screen.setFillColor(green);\n  Brain.Screen.drawRectangle(250, 10, 200, 100);\n  Brain.Screen.setFillColor(blue);\n  Brain.Screen.drawRectangle(250, 125, 200, 100);\n}\n\n// \"when LimitSwitchA pressed\" hat block\nvoid onevent_LimitSwitchA_pressed_0() {\n  if (Mode == 1.0) {\n    Mode = 2.0;\n    Brain.Screen.clearScreen();\n    message1.broadcast();\n  }\n}\n\n// \"when Brain screen pressed\" hat block\nvoid onevent_Brain_screen_pressed_1() {\n  if (Mode == 1.0) {\n    while (Brain.Screen.pressing()) {\n      if (coller == 1.0) {\n        Brain.Screen.setFillColor(blue);\n        Brain.Screen.setPenColor(blue);\n      } else if (coller == 2.0) {\n        Brain.Screen.setFillColor(green);\n        Brain.Screen.setPenColor(green);\n      } else if (coller == 3.0) {\n        Brain.Screen.setFillColor(red);\n        Brain.Screen.setPenColor(red);\n      } else {\n        Brain.Screen.setFillColor(white);\n        Brain.Screen.setPenColor(white);\n      }\n      Brain.Screen.drawRectangle(Brain.Screen.xPosition(), Brain.Screen.yPosition(), 50, 50);\n    wait(5, msec);\n    }\n  }\n}\n\n\nint main() {\n  // register event handlers\n  Brain.Screen.pressed(onevent_Brain_screen_pressed_0);\n  Brain.Screen.pressed(onevent_Brain_screen_pressed_1);\n  message1(onevent_message1_0);\n  LimitSwitchA.pressed(onevent_LimitSwitchA_pressed_0);\n\n  wait(15, msec);\n  // post event registration\n\n  // set default print color to black\n  printf(\"\\033[30m\");\n\n  // wait for rotation sensor to fully initialize\n  wait(30, msec);\n\n  whenStarted1();\n}","target":"Physical"}